{
    "name" : "Simple Ad Blocker",
    "version" : "1.0.0",
    "manifest_version" : 3,
    "description" : "A Simple and basic Ad blocker for websites",
    "icons" : {
        "128" : ""
    },
    "permissions" : [
        "declarativeNetRequest"
    ],
    "host_permissions" : [
        "<all_urls>"
    ],
    "background" : {
        "service_worker" : "Background.js"
    },
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["Content.js"]
        }
    ]
}